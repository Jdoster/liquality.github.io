<% pages = sitemap.resources.find_all{|p| p.content_type.include? "html"} %>
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
<% pages.each do |p| %>
  <url>
    <loc>https://liquality.io/<%=p.destination_path.gsub(/index.html/,'')%></loc>
    <priority><%=p.data.priority or '0.7'%></priority>
  </url>
<% end %>
</urlset>
